TerritoryManager        -type EXP       
WindowManager           -type EXP

#
# RISC_OSLib, or the Shared C Library...
#
SharedCLibrary          -type EXP -options SCL_APCS="-APCS 3/26bit"

#
# Libraries...
#
AsmUtils                -type EXP
TCPIPheaders            -type EXP
socklib
inetlib
unixlib
