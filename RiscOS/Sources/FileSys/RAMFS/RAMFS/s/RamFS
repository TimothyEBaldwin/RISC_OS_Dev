;>RamFS

; This GETs all relevant files for RAMFS
; This expects to be AASMed in the parent of directory asm
; which is expected to contain all the sources.

        GBLL    test_version
test_version SETL {FALSE}
        GET     VersionASM
        GET     Hdr:ListOpts
        GET     Hdr:Macros
        GET     Hdr:System
        GET     Hdr:Services
        GET     Hdr:ModHand
        GET     Hdr:UpCall
        GET     Hdr:Tokens
        GET     Hdr:Proc
        GET     Hdr:MsgTrans
        GET     Hdr:FileCore
        GET     Hdr:FSNumbers
        GET     Hdr:NewErrors
        GET     Hdr:PublicWS
        GET     Hdr:OSRSI6

        GET     MyMacros.s
        GET     Defns.s
        GET     DebugOpts.s
        GET     Header.s
        GET     Errors.s
        GET     RamFS00.s
        GET     RamFS05.s
        GET     RamFS15.s
        GET     TokHelpSrc.s
        GET     RamFS50.s
        GET     MsgCode.s

 [ Debug
        InsertDebugRoutines
 ]

        END
