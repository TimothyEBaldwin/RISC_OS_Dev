       |
         DCB    FILINGSYSTEMDEPENDENT + HELPISCODE

         DCD    0
         DCD    free_help - startofheader
       ]

;-----------------------------------------------------------------------------------------------
; ENTRY FOR KEYWORD '*LOCK'
;-----------------------------------------------------------------------------------------------

         ALIGN
         DCB    "Lock", 0
         ALIGN

         DCD    Lock - startofheader

         DCB    0
         DCB    0
         DCB    1
       [ LazyIntHelp
         DCB    FILINGSYSTEMDEPENDENT + (International_Help :SHR: 24)

         DCD    lock_syntax - startofheader
