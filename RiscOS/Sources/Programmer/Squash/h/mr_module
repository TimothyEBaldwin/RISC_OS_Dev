typedef struct mr_regset
{
        int     r0;
        int     r1;
        int     r2;
        int     r3;
        int     r4;
        int     r5;
        int     r6;
        int     r7;
        int     r8;
        int     r9;
        int     r10;
        int     r11;
        int     r12;
}       mr_regset;

typedef struct mr_error
{
        int     errno;
        char    errmess[ 252 ];
}       mr_error;
