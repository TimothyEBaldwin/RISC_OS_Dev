                TTL     ==> &.Arthur.Econet.Econet

              [ :LNOT::DEF:OldOs
                GBLL    OldOs
OldOs           SETL    {FALSE}
              ]

              [ OldOs
                ; RISC OS 2
                GBLL    UseMsgTrans
UseMsgTrans     SETL    {FALSE}
                GBLS    OsString
OsString        SETS    " (2.00)"
                GBLL    StrongARM
StrongARM       SETL    {FALSE}
                ! 0, "Assembling for RISC OS 2"
              |
                ; RISC OS 3
                GBLL    UseMsgTrans
UseMsgTrans     SETL    {TRUE}
                GBLS    OsString
OsString        SETS    ""
                GBLL    StrongARM
StrongARM       SETL    {TRUE}
                ! 0, "Assembling for RISC OS 3"
              ]

                GBLL    ARM810support
ARM810support   SETL    {FALSE}
                GBLL    ReleaseVersion
ReleaseVersion  SETL    {TRUE}

                LNK     Module.s
