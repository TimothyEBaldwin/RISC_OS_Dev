#include <stdio.h>
#include "syscalls.h"
#include <linux/mman.h>
#include <linux/sched.h>
#include <linux/fs.h>
#include <linux/stat.h>
#include <linux/errno.h>
#include <linux/limits.h>
#include <linux/signal.h>

#define DEF2(name, value) printf("%s * 0x%x\n", name, value);
#define DEF(x) DEF2(#x, x)

int main(void) {

  DEF(PROT_NONE)
  DEF(PROT_READ)
  DEF(PROT_WRITE)
  DEF(PROT_EXEC)

  DEF(MAP_SHARED)
  DEF(MAP_PRIVATE)
  DEF(MAP_FIXED)
  DEF(MAP_ANONYMOUS)
  DEF(MAP_NORESERVE)

  DEF(CLONE_VM)

  DEF(SIG_BLOCK)
  DEF(SIG_UNBLOCK)
  DEF(SIG_SETMASK)

  DEF2("sizeof_struct_stat", sizeof(struct stat))
  DEF2("struct_stat_mode", offsetof(struct stat, st_mode))
  DEF2("struct_stat_size", offsetof(struct stat, st_size))

#include "syscall_list.h"

  puts("\n END");

  return 0;
}
